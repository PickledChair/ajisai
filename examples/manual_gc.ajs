func print_repeated_str(src: str, count: i32) {
    println_str(str_slice(src, 0, count));
    if count != 0 {
        print_repeated_str(src, count - 1)
    } else {
        ()
    }
}

func main() {
    print_repeated_str("Hoge", 4);
    gc_start();
    print_repeated_str("Fuga", 4);
    gc_start();
    print_repeated_str("Piyo", 4)
}
